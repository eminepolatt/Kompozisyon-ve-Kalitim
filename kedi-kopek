
package kompozisyon_kalitim;
class Kedi{
    protected int ayakSayisi=4;
        public void yakalaAv(){
        System.out.println("Kedi sınıfı av yakaladı");
    }
        public static void main(String[] args) {
        Kedi kd=new Kedi();
        kd.yakalaAv();
    }
}
class Kaplan extends Kedi{ //KAPLAN kedinin özelliklerini kullanabilir
    public static void main(String[] args) {
        Kaplan kp=new Kaplan();
        kp.yakalaAv();
        System.out.println("Ayak sayısı="+kp.ayakSayisi);
    }
    
}
